<%- @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.gender %></td>
      <td><%= user.birthdate %></td>
      <td><%= user.email %></td>
      <td>
        <%= link_to  "Show", user_path(user), :class => "btn btn-primary" %>
      </td>
    </tr>
<% end %>